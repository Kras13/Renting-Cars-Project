@model CarDetailsViewModel

<div class="row">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Make</th>
                <th scope="col">Model</th>
                <th scope="col">Year</th>
                <th scope="col">Description</th>
            </tr>
        </thead>
        <tbody>
            {
            <tr>
                <td>@Model.Make</td>
                <td>@Model.Model</td>
                <td>@Model.Year</td>
                <td>@Model.Description</td>
                <td></td>
            </tr>
            }
        </tbody>
    </table>
</div>
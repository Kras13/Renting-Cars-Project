@using CarRentingSystem.Areas.Admin.Service
@model IEnumerable<ScheduleModel>


<div class="row">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Make</th>
                <th scope="col">Model</th>
                <th scope="col">Year</th>
                <th scope="col">Information</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var car in Model)
            {
                <tr>
                    <td>@car.Make</td>
                    <td>@car.Model</td>
                    <td>@car.Year</td>
                    <td>@car.Information</td>
                </tr>
            }
        </tbody>
    </table>
</div>